{
  "name": "react-svg-polygon",
  "version": "2.0.0",
  "description": "A simple component for rendering polygons",
  "keywords": [
    "polygon",
    "polyline",
    "shape",
    "react",
    "reactjs"
  ],
  "main": "package-publish/Poly.js",
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "16.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-react": "^7.0.0",
    "jest": "^24.0.0",
    "jest-dom": "^3.0.1",
    "parcel-bundler": "^1.11.0",
    "react-dom": "^16.7.0",
    "react-testing-library": "^5.4.4"
  },
  "scripts": {
    "start": "npm run build-dev && npx parcel demo/index.html",
    "build-dev": "npx parcel build src/Poly.js --out-dir ./demo --no-minify",
    "test": "npx jest --watchAll",
    "eject": "npx react-scripts eject",
    "build-prod": "npx parcel build src/Poly.js --out-dir ./package-publish"
  },
  "author": "Kevin Brock <kbrock092684@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/kbrock84/react-svg-polygon",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
